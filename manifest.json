{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "background": {
    "scripts": [
      "ProfileHandler.js",
      "CookieManager.js",
      "background.js"
      ]
  },

  "browser_action": {
    "default_icon": "Resources\\icon.png",
    "default_popup": "HTML\\BasePage.html"
  },
  "permissions": [
    "cookies",
    "http://*/*",
    "https://*/*"
  ]
}
